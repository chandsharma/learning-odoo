<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="inheritgenadd_tree">
            <field name="name">inheritgenadd.tree</field>
            <field name="model">hospital.patients</field>
            <field name="inherit_id" ref="patient_list_view"/>
            <field name="arch" type="xml">
                <tree position="inside">
                    <field name="gender" />
                    <field name="address" />
                  </tree>
            </field>
        </record>
    </data>
    <data>
        <record model="ir.ui.view" id="inheritgenadd_form">
            <field name="name">inheritgenadd.form</field>
            <field name="model">hospital.patients</field>
            <field name="inherit_id" ref="patient_form_view"/>
            <field name="arch" type="xml">
                <form position="inside">
                <sheet>
                 <group>
                    <field name="gender" />
                    <field name="address" />
                    
                    </group>
                    </sheet>
                  </form>
            </field>
        </record>
    </data>

